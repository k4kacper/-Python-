+def dodaj_uzytkownika(uzytkownik: Uzytkownik):
+    if any(u.email == uzytkownik.email for u in uzytkownicy_db):
+        raise HTTPException(status_code=400, detail="E-mail ju≈º istnieje!")
+
+    uzytkownicy_db.append(uzytkownik)
